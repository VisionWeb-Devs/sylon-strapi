{
  "kind": "collectionType",
  "collectionName": "variants",
  "info": {
    "singularName": "variant",
    "pluralName": "variants",
    "displayName": "Variant"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "variants"
    },
    "sku": {
      "type": "string"
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "stock_quantity": {
      "type": "integer",
      "min": 0,
      "required": true
    },
    "images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    }
  }
}
